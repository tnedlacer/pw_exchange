<div class="panel panel-default pw_response" data-id="<%= pw_response.id %>">
  <div class="panel-heading" data-toggle="collapse" href="#collapse_<%= pw_response.id %>">
    <h4 class="panel-title">
      <span class="glyphicon glyphicon-lock"></span>
      <%= pw_response.code %>
      <small class="badge pull-right"><%= l(pw_response.created_at, format: :default) %></small>
    </h4>
  </div>
  <div id="collapse_<%= pw_response.id %>" class="panel-collapse collapse">
    <div class="panel-body pw_response_content"><%= t("text.please_authenticate") %></div>
  </div>
</div>